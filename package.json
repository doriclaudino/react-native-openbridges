{
  "name": "openbridges",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "rebuild": "npm run bug && react-native run-android",
    "stop-activity": "awk '{ match($0, /package\\=\"(.+\\>)/, arr); if(arr[1] != \"\") {system(\"adb shell am force-stop \" arr[1])} }' ./android/app/src/main/AndroidManifest.xml",
    "start-activity": "awk '{ match($0, /package\\=\"(.+\\>)/, arr); if(arr[1] != \"\") {system(\"adb shell am start -n \" arr[1] \"/.MainActivity\")} }' ./android/app/src/main/AndroidManifest.xml",
    "reopen": "npm run stop-activity && npm run start-activity",
    "bug": "rimraf ./android/app/build/generated/res/google-services/debug"
  },
  "dependencies": {
    "frisbee": "^2.0.5",
    "moment": "^2.24.0",
    "react": "16.6.3",
    "react-moment": "^0.8.4",
    "react-native": "0.58.4",
    "react-native-country-picker-modal": "^0.7.1",
    "react-native-firebase": "^5.2.2",
    "react-native-form": "^2.1.3",
    "react-native-gesture-handler": "^1.0.15",
    "react-native-modal-datetime-picker": "^6.0.0",
    "react-native-paper": "^2.11.0",
    "react-native-phone-input": "^0.2.1",
    "react-native-vector-icons": "^6.3.0",
    "react-navigation": "^3.2.3",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "jest": "24.1.0",
    "metro-react-native-babel-preset": "0.51.1",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
